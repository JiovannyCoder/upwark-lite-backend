{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
      {
        "handle": "middleware",
        "src": "/",
        "method": "OPTIONS",
        "middleware": [
          {
            "name": "cors",
            "options": {
              "origin": ["https://upwark-lite-frontend.vercel.app"]
            }
          }
        ]
      },
      {
          "src": "/(.*)",
          "dest": "server.js"
      }
  ]
}